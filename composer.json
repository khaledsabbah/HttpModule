{
  "name": "khaledsabbah/http-module",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Idaratech\\Integrations\\": "src/"
    }
  },
  "description": "HTTP integration helpers (requests, responses, middlewares, logging, enums, validation) for Laravel apps.",
  "keywords": [
    "laravel",
    "http-client",
    "sdk",
    "integrations",
    "guzzle",
    "validation",
    "enums"
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/validation": "^10.0|^11.0",
    "archtechx/enums": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Idaratech\\Integrations\\IntegrationsServiceProvider"
      ]
    }
  }
}